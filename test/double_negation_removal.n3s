@prefix : <urn:example:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

() log:nand {
    () log:nand {
        :test1 a :test .
    } .
} .

() log:nand {
    (skolem:x) log:nand {
        :test2 a :test .
    } .
} .

() log:nand {
    :subject1 a :subject .

    () log:nand {
        () log:nand {
            :test3 a :test .
        } .
    } .
} .

() log:nand {
    :subject1 a :subject .

    (skolem:y) log:nand {
        (skolem:x) log:nand {
            :test4 a :test .
        } .
    } .
} .

{
    :test1 a :test .
    :test2 a :test .

    () log:nand {
        :subject1 a :subject .
        :test3 a :test .
    } .

    () log:nand {
        :subject1 a :subject .
        :test4 a :test .
    } .
}
log:query
{
    :test :is true .
} .
