@prefix : <urn:example:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

# q -> s
() log:nand {
    :q a :verb .
    () log:nand {
        :s a :verb . 
    } .
} .

# r -> s
() log:nand {
    :r a :verb .
    () log:nand {
        :s a :verb . 
    } .
} .

# (q -> s) , (r -> s) / (q v r) -> s
() log:nand {
    # (q v r) -> s
	() log:nand {
	    () log:nand {
	        () log:nand {
	            :q a :verb .
	        } .
	        () log:nand {
	            :r a :verb .
	        } . 
	    } .
	    () log:nand {
	        :s a :verb .
	    } .
	} .
    () log:nand {
        :test :is true .
    } .
} .

{ :test :is true } log:query { :test :is true } .
