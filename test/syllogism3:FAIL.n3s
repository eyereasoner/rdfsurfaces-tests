@prefix : <urn:example:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

:Alice a :Cyclist .
() log:nand {
    :Alice a :IceSkater .
} .

# All cyclists are iceskaters and all iceskaters are cyclists
(_:X) log:nand {
    _:X a :Cyclist .
    () log:nand {
        _:X a :IceSkater .
    } .
} . 

(_:X) log:nand {
    _:X a :IceSkater .
    () log:nand {
        _:X a :Cyclist .
    } .
} . 
