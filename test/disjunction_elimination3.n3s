@prefix : <urn:example:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

# Testing if we don't eliminate too much 
# Skip this when reasoning only allows disjunction via contrapositives

:A a :B .
() log:nand {
    :A a :B2 .
} .

# ∀S : (S a B) -> (S a B1) ∨ (S a B2)
(_:S) log:nand {
    _:S a :B .

    () log:nand {
      () log:nand {
        () log:nand {
            _:S a :B1 .
        } .

        () log:nand {
            _:S a :B2 .
        } .
      } .
    } .
} .

# ∀S : (S a B1) -> (S a C) 
(_:S) log:nand {
    _:S a :B1 .

    () log:nand {
        _:S a :C .
    }.
} .

# ∀S : (S a B2) -> (S a C) 
(_:S) log:nand {
    _:S a :B2 .

    () log:nand {
        _:S a :C .
    }.
} .

# Test if we still have a particular (A a B1) ∨ (A a B2)
() log:nand {
    () log:nand {
        () log:nand {
            :A a :B1 .
        } .
        () log:nand {
            :A a :B2 .
        } .
    } .
    () log:nand {
        :A a :D .
    } .
} .

() log:nand {
    # Test if we still get the C
    :A a :C .
    # Test if we still get the disjunction
    :A a :D .

    () log:nand {
        :test :is true .
    } .
} .

# Test
() log:nand { :test :is true. () log:nans { :test :is true } } .
