@prefix : <urn:example:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

:Alice a :Girl .

# \Every x : \Some y : x a Girl & y a Boy & y loves x
(_:X) log:nand {
   (_:Y) log:nand {
        _:X a :Girl .
        _:Y a :Boy .
        _:Y :loves _:X .
   } . 
} .

(_:X _:Y) log:nand {
    _:X :loves _:Y .
    () log:nand {
        :test :is true .
   } .
} .

{ :test :is true } log:query { :test :is true } .
