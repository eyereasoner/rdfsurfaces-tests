@prefix : <urn:example:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

:Alice :loves :Wine .

# This should not give a contradiction
() log:nand {
    :Alice :loves :Wine .
    # An empty graph in N3 means true
    # We thus say:
    #    ¬((Alice loves Wine) ∧ ¬true ) 
    #       = ¬(Alice loves Wine) ∨ true
    #       = true
    () log:nand {} .
} .

# Test
() log:nand {
    :Alice :loves :Wine .
    () log:nand {
        :test :is true .
    } .
} .

() log:nand { :test :is true. () log:nans { :test :is true } } .
