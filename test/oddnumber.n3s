@prefix : <urn:example:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .
@prefix math: <http://www.w3.org/2000/10/swap/math#> .

10 a :NaturalNumber .

# All x : (X a EvenNumber) OR (X a OddNumber)
(_:x) log:nand {
    () log:nand {
        _:x a :EvenNumber .
    } .
    () log:nand {
        _:x a :OddNumber .
    } .
} . 

# All x,y : (X a NaturalNumber) ->  (y = x-1, y > 0 a NaturalNumber)
(_:x _:y) log:nand {
    _:x a :NaturalNumber .

    (_:x -1 ) math:sum _:y .

    _:y math:greaterThan 0 .

    () log:nand {
        _:y a :NaturalNumber .
    } .
} . 

# All x : (X a NaturalNumber) -> ( x : 2 = 0 , not(X a OddNumber) )
(_:x) log:nand {
    _:x a :NaturalNumber .

    (_:x 2) math:remainder 0 .

    () log:nand {
      () log:nand {
        _:x a :OddNumber .
      } .
    } .
} .

(_:S _:O) log:nand {
    _:S a _:O .
    () log:nans {
        _:S a _:O .
    } .
} .

(_:S _:O) log:nand {
    () log:nand {
        _:S a _:O .
    } .
    () log:nans {
        () log:nand {
            _:S a _:O .
        } .
    } .
} .

# Test
() log:nand {
    10 a :EvenNumber .
    8 a :EvenNumber .
    6 a :EvenNumber .
    4 a :EvenNumber .
    2 a :EvenNumber .
    () log:nand {
        :test :is true .
    } .
} .

() log:nand { :test :is true. () log:nans { :test :is true } } .
