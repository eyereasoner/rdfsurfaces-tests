@prefix : <urn:example:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

# Premises

# p -> q 

() log:nand {
    :p a :verb .
    () log:nand {
        :q a :verb .
    } .
} .

# p v q 
() log:nand {
    () log:nand {
        :p a :verb . 
    } .
    () log:nand {
        :q a :verb . 
    } .
} .

# Consequent

# q
() log:nand {
    :q a :verb .
    () log:nand {
        :test :is true . 
    } .
} .

# query
() log:nand { :test :is true. () log:nans { :test :is true } } .
