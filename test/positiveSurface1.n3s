@prefix : <urn:example:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

_:x a :PastaEater .

# The negative of a negative surface is the implicit positive surface.
# We can write grafiti on this surface and just add more triples on 
# the positive surface.
(_:x) log:nand {
    () log:nand {
        _:x a :RiceEater .

        (_:x _:y) log:nand {
            _:x a :PastaEater . 
            _:y a :RiceEater .

            (_:z) log:nand {
                _:z a :RisottoEater .
            } .
        } .
    } .
} .  

(_:S _:O) log:nand {
    _:S a _:O .
    () log:nans {
        _:S a _:O .
    } .
} .

# Test

(_:x) log:nand {
    _:x a :RisottoEater .
    () log:nand {
        :test :is true .
    } .
} .

() log:nand { :test :is true. () log:nans { :test :is true } } .
