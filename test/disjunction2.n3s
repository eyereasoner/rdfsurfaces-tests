@prefix : <urn:example:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

# Testing to make a disjunction based on generated data
# 1 We first infer that Alice and Bob are students
# 2 Then we say that students are from Ghent or Leuven
# 3 Then we say that Ghent and Leuven students are smart

:Alice a :PhdStudent ;
    a :GhentStudent .
:Bob a :MasterStudent ;
    a :LeuvenStudent .

# [1]
(_:S) log:nand {
    _:S a :MasterStudent .

    () log:nand {
        _:S a :Student .
    } .
} .

(_:S) log:nand {
    _:S a :PhdStudent .

    () log:nand {
        _:S a :Student .
    } .
} .

# [2]
(_:S) log:nand {
    _:S a :Student .

    () log:nand {
        _:S a :GhentStudent .
    } .

    () log:nand {
        _:S a :LeuvenStudent .
    } .
} .

# [3]
(_:S) log:nand {
    _:S a :GhentStudent .

    () log:nand {
        _:S a :SmartPerson .
    } .
} .

(_:S) log:nand {
    _:S a :LeuvenStudent .

    () log:nand {
        _:S a :SmartPerson .
    } .
} .

(_:S _:O) log:onQuerySurface {
    _:S a _:O .
} .

# Test
() log:nand {

    :Alice a :SmartPerson .

    () log:nand {
        :testAlice :is true .
    } .
} .

() log:nand {
    :Bob a :SmartPerson .

    () log:nand {
        :testBob :is true .
    } .
} .

() log:nand {
    :testAlice :is true .
    :testBob :is true .
    () log:nand {
        :test :is true .
    } .
} .

() log:nand { :test :is true. () log:nans { :test :is true } } .
