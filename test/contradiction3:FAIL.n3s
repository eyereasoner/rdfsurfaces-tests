@prefix : <http://example.org/ns#> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

# \Some y : not( y log:nand { :Alice :hates :Quiche } )
() log:nand {
    # The negative surface below is just an RDF structure (not a negation)
    _:y log:nand {
        :Alice :hates :Quiche
    } .
} .

# \Some y : y log:nand { :Alice :hates :Quiche }
# The negative surface below is just an RDF structure (not a negation)
_:y log:nand {
    :Alice :hates :Quiche .
} .
