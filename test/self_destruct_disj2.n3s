@prefix : <urn:example:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

# Self destruct or Beetle is a Car
() log:nand {
    () log:nand {
        # The self destructor
        (_:G _:F) log:nand {
            _:G log:nand _:F .

            () log:nand {
                () log:nand {
                    _:G log:nand _:F .
                } .
            } .
        } .
    } .
    () log:nand {
        :Beetle a :Car .
    } .
} .

() log:nand {
    :Beetle a :Car .

    () log:nand {
        :test :is true .
    } .
} .

() log:nand { :test :is true. () log:nans { :test :is true } } .
