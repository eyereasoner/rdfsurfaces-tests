@prefix : <urn:example:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

# If there is a negative surface (including this surface)
# this surface is false, or Beetle is a Car
(_:G _:F) log:nand {
   _:G log:nand _:F .

   () log:nand {

       # Preparing the OR case
       () log:nand {
           # This surface was false
           _:G log:nand _:F .
        
           # A Beetle is a Car
           () log:nand {
                :Beetle a :Car .
           } .
       } .
   } . 
} .

