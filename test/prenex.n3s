@prefix : <urn:example:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

# (\Every x: (x a Cat)) v (:Hector a :Dog)
() log:nand {
    () log:nand {
        (_:X) log:nand {
           () log:nand {
              _:X a :Cat .
           } .
        } .
    } .

    () log:nand {
        :Hector a :Dog .
    } . 
} .

() log:nand{

    # \Every x: ( (x a Cat) v (:Hector a :Dog) )
    (_:X) log:nand {
        () log:nand {
            _:X a :Cat .
        } .
        () log:nand {
            :Hector a :Dog .
        } . 
    } .

    () log:nand {
        :test :is true .
    } .
} .

{ :test :is true } log:query { :test :is true } .
