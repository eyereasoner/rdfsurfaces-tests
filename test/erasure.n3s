@prefix : <urn:example:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

# Erasure test

# A
:A a :A0 .

# A . ~( B . ~A ) = A , whatever B is
# Testing we don't get a fuse
() log:nand {
    # B
    () log:nand true .

    # This we should get as output
    () log:nand {
        :A a :A0 .
    } .
} .

# A . ~(B . ~(C . ~A)) = A . ~B , whatever C is
# 
# Make C something false...
#  `-> we should get a ~B
() log:nand {
    :B a :B1 .

    () log:nand {
        # C
        () log:nand true .

        () log:nand {
            :A a :A0 .
        } .
    } . 
} .

# A . ~(B . ~(C . ~A)) = A . ~B , whatever C is
#
# Make C something true...
#  `-> we should get a ~B
() log:nand {
    :B a :B2 .

    () log:nand {
        # C
        :C a :C2 .

        () log:nand {
            :A a :A0 .
        } .
    } . 
} .

# A . ~(B . ~(C . ~(D . ~A))) = A . (B -> C), whatever D is
#
# Make a B
:B a :B3 .
# Make D something false...
#  `-> we should get a (B -> C)
() log:nand {
    :B a :B3 .

    () log:nand {
        # C
        :C a :C3 .

        () log:nand {
            # D .
            () log:nand true .

            () log:nand {
                :A a :A0 .
            } .
        } .
    } .
} .

# A . ~(B . ~(C . ~(D . ~A))) = A . (B -> C), whatever D is
#
# Make a B
:B a :B4 .
# Make D something true...
#  `-> we should get a (B -> C)
() log:nand {
    :B a :B4 .

    () log:nand {
        # C
        :C a :C4 .

        () log:nand {
            # D .
            :D a :D4 .

            () log:nand {
                :A a :A0 .
            } .
        } .
    } .
} .

# ~(B . ~(C . ~B)) = ~B, whatever C is
# Make C something false
() log:nand {
    :B a :B5 .

    () log:nand {
        # C
        () log:nand true .

        () log:nand {
            :B a :B5 .
        } .
    } .
} .

# ~(B . ~(C . ~B)) = ~B, whatever C is
# Make C something true
() log:nand {
    :B a :B6 .

    () log:nand {
        # C
        :C a :C6 .

        () log:nand {
            :B a :B6 .
        } .
    } .
} .

# ~(B . ~(C . ~(D . ~B))) = (B -> C), whatever D is
#
# Make a B
:B a :B7 .
# Make D something false...
#  `-> we should get (B -> C)
() log:nand {
    :B a :B7 .

    () log:nand {
        # C
        :C a :C7 .

        () log:nand {
            # D .
            () log:nand true .

            () log:nand {
                :B a :B7 .
            } .
        } .
    } .
} .

# ~(B . ~(C . ~(D . ~B))) = (B -> C), whatever D is
#
# Make a B
:B a :B8 .
:D a :D8 .
# Make D something true...
#  `-> we should get (B -> C)
() log:nand {
    :B a :B8 .

    () log:nand {
        # C
        :C a :C8 .

        () log:nand {
            # D .
            :D a :D8 .

            () log:nand {
                :B a :B8 .
            } .
        } .
    } .
} .

(_:S _:O) log:nand {
    _:S a _:O .
    () log:nans {
        _:S a _:O .
    } .
} .

(_:S _:O) log:nand {
    () log:nand {
        _:S a _:O .
    } .
    () log:nans {
        () log:nand {
            _:S a _:O .
        } .
    } .
} .

# Test
() log:nand {
    :A a :A0 .

    :C a :C3 .
    :C a :C4 .
    :C a :C7 .
    :C a :C8 .

    () log:nand {
        :B a :B1.
    } .

    () log:nand {
        :B a :B2 .
    } .

    () log:nand {
        :B a :B5 .
    } .

    () log:nand {
        :B a :B6 .
    } .

    () log:nand {
        :test :is true .
    } .
} .

() log:nand { :test :is true. () log:nans { :test :is true } } .
