@prefix : <urn:example:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

# ∀x : ~( Beetle a Car )
(_:x) log:nand {
    :Beetle a :Car .
} .

# ∀S,O : ~(S a O)
(_:S _:O) log:nand {
    () log:nand {
        _:S a _:O .
    } .
    () log:nand {
        :test :is true .
    } .
} .

{ :test :is true } log:query { :test :is true } .
