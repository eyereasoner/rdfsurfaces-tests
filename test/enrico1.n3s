@prefix : <urn:example:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

:jozef a :father.

:franklin a :father .

# \forall x: ( (x ref:type :father) -> \exists y (y :child x) )
(_:x) log:nand {
    _:x a :father .
    (_:y) log:nand {
        _:y :child _:x .
    } .
} .

# Test
(_:x _:y) log:nand {
    _:x :child :jozef .
    _:y :child :franklin .

    _:x log:notEqualTo _:y .

    () log:nand {
        :test :is true .
    } .
} .

# queries
{ :test :is true } log:query { :test :is true } .
