@prefix : <urn:example:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

# Modus tollens example.

# If X is a chicken, then X is a bird
(_:X) log:nand {
    _:X a :Chicken .
    () log:nand {
        _:X a :Bird .
    } .
} .

# Rex is not a bird...
() log:nand {
    :Rex a :Bird .
} .

# ..then Rex is not a chicken

# (test if this conclusion is true)
() log:nand {
    () log:nand {
        :Rex a :Chicken .
    } .
    () log:nand {
        :test :is true .
    } .
} .

() log:nand { :test :is true. () log:nans { :test :is true } } .

# Show the results
(_:S _:O) log:nand {
    () log:nand {
        _:S a _:O .
    } .
    () log:nans {
        () log:nand {
            _:S a _:O .
        } .
    } .
} .
