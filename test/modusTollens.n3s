@prefix : <urn:example:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

# Modus tollens example.

# If X is a chicken, then X is a bird
(_:X) log:nand {
    _:X a :Chicken .
    () log:nand {
        _:X a :Bird .
    } .
} .

# Rex is not a bird...
() log:nand {
    :Rex a :Bird .
} .

# ..then Rex is not a chicken

# (test if this conclusion is true)
() log:nand {
    () log:nand {
        :Rex a :Chicken .
    } .
    () log:nand {
        :test :is true .
    } .
} .

{ :test :is true } log:query { :test :is true } .

# Show the results
{
    () log:nand {
        ?S a ?O .
    } .
} 
log:query
{
    () log:nand {
        ?S a ?O .
    } .
} . 
