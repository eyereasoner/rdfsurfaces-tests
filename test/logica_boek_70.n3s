@prefix : <urn:example:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

# p -> q & r
() log:nand {
    :p a :verb .
    () log:nand {
        :q a :verb .
        :r a :verb .
    } .
} .

# p -> ~r
() log:nand {
    :p a :verb .
    :r a :verb .
} .

# it must follow: 
# ~p
() log:nand {
    () log:nand {
        :p a :verb .
    }.
    () log:nand {
        :test :is true .
    } .
} .

() log:nand { :test :is true. () log:nans { :test :is true } } .
