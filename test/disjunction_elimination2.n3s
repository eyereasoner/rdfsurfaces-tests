@prefix : <urn:example:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

:A a :B .
:C a :B .
:D a :B .
() log:nand {
    :A a :B2 .
} .
() log:nand {
    :A a :B3 .
} .
() log:nand {
    :C a :B1 .
} .
() log:nand {
    :C a :B3 .
} .
() log:nand {
    :D a :B1 .
} .
() log:nand {
    :D a :B2 .
} .


# This is an extension of disjunction_elimination.n3 which should conclude 
# that A ,C ,D are all :C-s.

# ∀S : (S a B) -> (S a B1) ∨ (S a B2) ∨ (S a B3) 
(_:S) log:nand {
    _:S a :B .

    () log:nand {
        _:S a :B1 .
    } .

    () log:nand {
        _:S a :B2 .
    } .

    () log:nand {
        _:S a :B3 .
    } .
} .

# ∀S : (S a B1) -> (S a C) 
(_:S) log:nand {
    _:S a :B1 .

    () log:nand {
        _:S a :C .
    }.
} .

# ∀S : (S a B2) -> (S a C) 
(_:S) log:nand {
    _:S a :B2 .

    () log:nand {
        _:S a :C .
    }.
} .

# ∀S : (S a B3) -> (S a C) 
(_:S) log:nand {
    _:S a :B3 .

    () log:nand {
        _:S a :C .
    }.
} .

# Test
() log:nand {
    :A a :C .
    :C a :C .
    :D a :C .
    () log:nand {
        :test :is true .
    } .
} .

() log:nand { :test :is true. () log:nans { :test :is true } } .
