@prefix : <urn:example:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

# Erasure test

# ~(B . ~(C . ~(B . D))) = B -> C . ~D
# Make a B
:B a :B2 .
() log:nand {
    :B a :B2 .

    () log:nand {
        # C
        :C a :C2 .

        () log:nand {
            :B a :B2 .
            :D a :D2 .
        } .
    } .
} .

# Verbose
(_:S _:O) log:nand {
    _:S a _:O .
    () log:nans {
        _:S a _:O .
    } .
} .

(_:S _:O) log:nand {
    () log:nand {
        _:S a _:O .
    } .
    () log:nans {
        () log:nand {
            _:S a _:O .
        } .
    } .
} .

# Test
() log:nand {
    :C a :C2 .

    () log:nand {
        :D a :D2 .
    } .

    () log:nand {
        :test :is true .
    } .
} .

() log:nand { :test :is true. () log:nans { :test :is true } } .
