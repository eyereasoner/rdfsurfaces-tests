#        _:Y a :Airplane .
#        _:X :id 1 .
#        _:Y :id 2 .
#        _:X owl:sameAs _:Y .
#    } .
#} .

(_:X _:Y) log:nand {
    # So this must be true
    () log:nand {
        _:X a :Airplane .
        _:Y a :Airplane .
        _:X :id 1 .
        _:Y :id 2 .
        _:X owl:sameAs _:Y .
    } .
    () log:nand {
        :test :is true .
    }
} .

() log:nand { :test :is true. () log:nans { :test :is true } } .

# OWL

(_:A _:B) log:nand {
    _:A owl:differentFrom _:B .
    () log:nand {
        _:B owl:differentFrom _:A .
    } . 
} . 

(_:A _:B _:C) log:nand {
    _:A owl:sameAs _:B .
    _:A owl:differentFrom _:B .
} .  

(_:A _:B _:C _:D) log:nand {
    _:A owl:sameAs _:B .
    _:A _:C _:D .
    () log:nand {
        _:B _:C _:D .
    } .
} .
    
(_:A _:B _:C _:D) log:nand {
    _:A owl:sameAs _:B .
    _:C _:A _:D .
    () log:nand {
        _:C _:B _:D .
    } .
} . 

(_:A _:B _:C _:D) log:nand {
    _:A owl:sameAs _:B .
    _:D _:C _:A .
    () log:nand {
        _:D _:C _:B .
    } .
} .

(_:A _:B) log:nand {
    _:A owl:sameAs _:B .
    () log:nand {
        _:B owl:sameAs _:A .
    } .
} .

(_:A _:B _:C) log:nand {
    _:A owl:sameAs _:B .
    _:B owl:sameAs _:C .
    () log:nand {
        _:A owl:sameAs _:C .
    } .
} .
